{
  "name": "projectacme",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "node-start": "nodemon ./bin/www",
    "build-css": "node-sass --include-path scss scss/main.scss public/stylesheets/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "parallelshell \"npm run node-start\" \"npm run watch-css\""
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-session": "^1.15.2",
    "hbs": "*",
    "mongoose": ">=5.7.5",
    "morgan": "~1.9.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "request": "*",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0"
  }
}
